citHeader("To cite dscore in publications use:")

citEntry(
  entry    = "Manual",
  title    = "D-score for Child Development",
  author   = as.person("S. van Buuren [aut], I. Eekhout [aut], A. Huizing [aut]"),
  year     = "2019",
  note     = "R package version 0.38",
  url      = "https://github.com/stefvanbuuren/dscore/tree/rename",
  key      = "dscore-package",
  textVersion =  "Van Buuren S, Eekhout I, Huizing A (2019). D-score for Child Development. The dscore R package, version 0.38."
)
